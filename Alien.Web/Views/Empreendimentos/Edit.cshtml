@model Domain.AlienDB_Empreendimentos

@{
    ViewData["Title"] = "Edit";
}



<script>
$('#num_of_comp').on('change',function(){
    if( $(this).val()==="1"){
    $("#div1").css("visibility", "visible");
     $("#div2,").css("visibility", "hidden");
    }
    else if( $(this).val()==="2"){
    $("#div2").css("visibility", "visible");
     $("#div1").css("visibility", "hidden");
    }
</script>

<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label style="visibility:hidden;" asp-for="Id" class="control-label"></label>
                <input style="visibility:hidden;" asp-for="Id" class="form-control" />
                <span asp-validation-for="Id" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label style="visibility:hidden;" asp-for="Id_Empresa" class="control-label"></label>
                <input style="visibility:hidden;" asp-for="Id_Empresa" class="form-control" />
                <span  asp-validation-for="Id_Empresa" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label style="visibility:hidden;" asp-for="Id_Tipo_Imovel" class="control-label"></label>
                <input style="visibility:hidden;" asp-for="Id_Tipo_Imovel" class="form-control" />
                <span asp-validation-for="Id_Tipo_Imovel" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="NomeLancamento" class="control-label"></label>
                <input asp-for="NomeLancamento" class="form-control" />
                <span asp-validation-for="NomeLancamento" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Cidade" class="control-label"></label>
                <input asp-for="Cidade" class="form-control" />
                <span asp-validation-for="Cidade" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Construtura" class="control-label"></label>
                <input asp-for="Construtura" class="form-control" />
                <span asp-validation-for="Construtura" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Id_Regiao" class="control-label"></label>
                <input asp-for="Id_Regiao" class="form-control" />
                <span asp-validation-for="Id_Regiao" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Save" class="btn btn-default" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Voltar</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
